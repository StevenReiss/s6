S6FILES= $(wildcard $(PRO)/s6/engine/src/*.s6)
TESTDIR = $(PRO)/s6/engine/src/tests.out

alltests.old: all
	rm -rf $(TESTDIR)
	mkdir $(TESTDIR)
	rm -rf /tmp/s6test/*
	for x in $(S6FILES); do \
	   echo work on $$x; \
	   djava -Xmx32000m edu.brown.cs.s6.engine.EngineMain -D -thread 1 $$x ; \
	done

alltests: $(notdir $(addsuffix .out,$(basename $(S6FILES))))

